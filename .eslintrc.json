{
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true
	},
	"extends": "eslint:recommended",
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 11
	},
	"parser": "babel-eslint",
	"ignorePatterns": ["**/examples/**/*.js"],
	"rules": {
		"no-unused-vars": "warn"
	}
}
